generator client {
  provider = "prisma-client-js"
  seed = "prisma/seed.ts"
}

datasource db {
  provider  = "postgresql"
  url  	    = env("DATABASE_URL")
}

model User {
  id          Int         @id @default(autoincrement())
  name        String  
  cpf         String      @unique
  email       String      @unique
  password    String  
  createdAt   DateTime    @default(now())
  updatedAt   DateTime    @updatedAt
}